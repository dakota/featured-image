{
    "name": "dakota/featured-image",
    "description": "Featured image plugin for Croogo",
    "type": "cakephp-plugin",
    "require": {
        "croogo/croogo": "^4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FeaturedImage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FeaturedImage\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
